[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    env,
    venv,
    .env,
    .eggs,
    *.egg,
    build,
    dist,
    migrations,
    staticfiles,
    media,
    logs,
    .pytest_cache,
    htmlcov,
    .coverage
per-file-ignores =
    __init__.py:F401
    migrations/*:E501 